<Window x:Class="MFCSoftware.AddChannelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MFCSoftware"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="增加通道" Height="200" Width="300">
    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <StackPanel Orientation="Horizontal" Margin="8">
            <Label>地址</Label>
            <TextBox Width="100" Text="{Binding Address}"></TextBox>
        </StackPanel>
        <Button Height="32" Margin="8" Click="Button_Click">确定</Button>
    </StackPanel>
</Window>
